<br>
<div>
    <h2 style="
        text-align: center;
        color:#ffc107; 
        background-color: #212529; 
        padding-bottom: 8px; 
        margin-bottom: 0px;
        border-radius: 10px 10px 0px 0px;"> Lista de Productos</h2>
    <% if (productos.length > 0){ %> 
        <table class="table table-dark table-hover">
            <tr><th>Producto</th><th>Precio</th><th>Imágen</th><th></th></tr>
            <%for(productos of productos){ %>
             <tr>
                 <td>- <%=productos.nombre %>  </td>
                 <td>$<%=productos.precio  %>  </td>
                 <td><img style="width: 30px; height: 30px;" src= <%=productos.url  %> ></td>
                 <td><button class="btn-warning" id="delete"> X </button></td>
             </tr>
            <% } %> 
        </table>
    <% } else { %>
        <p class="aler alert-warning">No se encontró productos</p>
    <% } %>
</div>